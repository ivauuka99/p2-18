#include <stdio.h>
#include <stdlib.h>
int main (){
	int n,m,i,j;
	float sr=0;
	scanf ("%d%d",&n,&m);
	int A[n][m];
	for (i=0;i<n;i++)
		for (j=0;j<m;j++)
			scanf ("%d",&A[i][j]);
	for (i=0;i<n;i++)
	{
		for (j=0;j<m;j++)
			printf ("%d ",A[i][j]);
		printf("\n");
	}
	for (i=0;i<n;i+=2)
	{
		sr=0;
		for (j=0;j<m;j++)
		{
			sr+=A[i][j];
		}
		sr/=m;
		printf ("Stroka: %d sr.znach: %.2f\n",i,sr);
	}	
}
